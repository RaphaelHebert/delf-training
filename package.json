    "@radix-ui/themes": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.23.0",
    "yup": "^1.4.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.1",
